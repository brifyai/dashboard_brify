# Configuración de Supabase
# Obtén estas credenciales desde tu proyecto en https://supabase.com/dashboard
REACT_APP_SUPABASE_URL=https://your-project-id.supabase.co
REACT_APP_SUPABASE_ANON_KEY=your-anon-key-here

# Configuración opcional para desarrollo
REACT_APP_ENVIRONMENT=development
REACT_APP_DEBUG=true

# Configuración de performance
REACT_APP_DEFAULT_PAGE_SIZE=50
REACT_APP_MAX_USERS_PER_PAGE=200
REACT_APP_CACHE_TIMEOUT=300000

# Configuración de monitoreo
REACT_APP_ENABLE_PERFORMANCE_MONITORING=true
REACT_APP_ENABLE_ERROR_REPORTING=true